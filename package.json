{
  "name": "nudeforge",
  "version": "1.0.0",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "NODE_ENV=development node src/app.js",
    "prod": "NODE_ENV=production node src/app.js",
    "postinstall": "npm rebuild sharp",
  "deploy": "chmod +x deploy.sh && ./deploy.sh",
  "lint": "eslint \"src/**/*.js\" --max-warnings=0",
  "lint:fix": "eslint \"src/**/*.js\" --fix",
  "test": "powershell -Command \"$env:NODE_ENV='test'; $env:SKIP_WEBSOCKET='true'; $env:SKIP_CAROUSEL_THUMBS='true'; $env:SKIP_QUEUE_PROCESSING='true'; node --experimental-vm-modules test/sharedLogger.test.mjs; node --experimental-vm-modules test/progressUI.test.mjs\""
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "img-comparison-slider": "^8.0.6",
    "multer": "^2.0.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "svg-captcha": "^1.4.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "selfsigned": "^2.4.1"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "@eslint/js": "^9.9.0",
    "globals": "^15.9.0"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=10.0.0"
  }
}
